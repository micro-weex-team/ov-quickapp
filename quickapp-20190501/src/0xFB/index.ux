		isswing: true,
		isshowswingDialog: false,
		isshowDialog: false,
		animateClass: '',
		minute:0,
		hours:0,
		openId: '',
		isGeardialog:false,
		timeoutInter:'',
		iswarm:true,
		isSwitch: false,
		accessToken: '',
		listtime: [],
