		}, {
			name: "baby",
			value: "宝宝风",
			data: "wind-baby-"
		}, {
			name: "mute",
			value: "静音风",
			data: "wind-mute-"
		}],
		mode: [{
			name: "normal",
			value: "正常风",
			data: "wind-normal-"
		}, {
			name: "sleep",
			value: "睡眠风",
			data: "wind-sleep-"
		}],
		//摇头角度
		swing_angle: [{
			name: "30",
