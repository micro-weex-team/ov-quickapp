					}).catch((error) => {
						that.isLanInit = false;
					});
				},
				closeWebsocket(e){
					let that = this;
					that.isclosews = true;
					if(that.timeoutInter != ''){
						clearInterval(that.timeoutInter);
					}
					if(that.ws != ''){
						that.ws.close({
							code: 1000,
							reason: '退出关闭',
							success: function() {
								that.ws = '';
								console.log("关闭websocket成功")
							},
							fail: function(data, code) {
								console.log("关闭websocke失败, code = "+JSON.stringify(code))
							}
						})
					}
				},
				onHide(){
					let that = this;
					that.istips = false;
					that.closeWebsocket();
