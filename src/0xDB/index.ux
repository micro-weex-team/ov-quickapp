        <!-- <div class="nav_ac" style="background-color: {{col == 'on'?Color:bgColor}};"> -->
			<refresh class="B6_refresh" offset="{{isoffset}}" onrefresh="scrollbottom" refreshing="{{isRefreshing}}">
