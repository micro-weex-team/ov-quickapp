			modeList:[
				{ isShow:true, work_mode: 'hot_wind_bake',name:'热风对流',iconSrc:'hot_wind@3x.png',tempArr:getTemp(50,250,5)},
				{ isShow:true, work_mode: 'underside_tube',name:'下管烧烤',iconSrc:'bottom_heat@3x.png',tempArr:getTemp(50,235,5)},
				{ isShow:true, work_mode: 'core_baking',name:'双上管烧烤',iconSrc:'double_grill@3x.png',tempArr:getTemp(100,235,5)},
				{ isShow:true, work_mode: 'double_upside_tube_fan',name:'双上管+风扇',iconSrc:'double_grill_and_fan@3x.png',tempArr:getTemp(100,250,5)},

				{ isShow:false, work_mode: 'double_tube_fan',name:'上下烧烤+风扇',iconSrc:'bottom_heat@3x.png',tempArr:getTemp(50,235,5)},
				{ isShow:false, work_mode: 'double_tube',name:'上下烧烤',iconSrc:'bottom_heat@3x.png',tempArr:getTemp(50,235,5)},
				{ isShow:false, work_mode: 'total_baking',name:'红外烧烤',iconSrc:'bottom_heat@3x.png',tempArr:getTemp(50,235,5)},
				{ isShow:false, work_mode: 'underside_tube_hot_wind_bake',name:'下管烧烤+热风对流',iconSrc:'bottom_heat@3x.png',tempArr:getTemp(50,235,5)},
