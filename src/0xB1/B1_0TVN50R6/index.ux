			// switch (mode) {
			// 	case 'hot_wind_bake':
			// 		that.modeText = '热风对流';
			// 		break;
			// 	case 'underside_tube':
			// 		that.modeText = '下管烧烤';
			// 		break;
			// 	case 'core_baking':
			// 		that.modeText = '双上管烧烤';
			// 		break;
			// 	case 'double_upside_tube_fan':
			// 		that.modeText = '双上管+风扇';
			// 		break;

			// 	case 'double_tube_fan':
			// 		that.modeText = '上下烧烤+风扇';
			// 		break;
			// 	case 'double_tube':
			// 		that.modeText = '上下烧烤';
			// 		break;
			// 	case 'total_baking':
			// 		that.modeText = '红外烧烤';
			// 		break;
			// 	case 'underside_tube_hot_wind_bake':
			// 		that.modeText = '下管烧烤+热风对流';
			// 		break;
			// 	case 'zymosis':
			// 		that.modeText = '发酵';
			// 		break;
			// 	default:
			// 		that.modeText = '--';
			// 		break;
			// }
			return that.modeText;
		},
		//工作状态过滤
		statusData(str) {
			let data = '待机中';
			if(str=='work' && this.pre_heat == 'on'){
				data = '预热中';
				return data;
			}
			switch (str) {
				case 'cancel':
					data = '待机中';
					break;
				case 'work':
					data = '运行中';
					break;
				case 'pause':
					data = '暂停中';
					break;
				case 'save_power':
					data = '省电';
					break;
