	/*
	温度选择
	*/
	function getTemp(start,end,step){
		var arr=[], b = [];
		if(start > end){
			return arr;
		}
		if(start<100){
			b.push({id:1,data:start});
		}
		for(var i=0;start+i*step <= end;i++){
			var value = start+step*i;
			if(value<100){
				continue;
			}
			let obj = {
				id: i,
				data: value
			}
			arr.push(obj);
		}
		return b.concat(arr);
	}

	export default {
